{
    "main": "bin/phpdoc",
    "output": "build/phpDocumentor.phar",
    "files": [
        "config/bundles.php",
        "VERSION",
        "vendor/phpdocumentor/reflection-docblock/src/DocBlock/Tags/Author.php"
    ],
    "directories-bin": [
        "data",
        "var/cache/prod"
    ],
    "git-version": "package_version",
    "chmod": "0755",
    "compression": "GZ",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ]
}
