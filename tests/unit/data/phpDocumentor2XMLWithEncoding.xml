<phpdocumentor>
    <parser>
        <default-package-name>Default</default-package-name>
        <encoding>ISO-8859-1</encoding>
    </parser>
    <files>
        <directory>src</directory>
    </files>
    <transformer>
        <target>build/docs</target>
    </transformer>
    <logging>
        <level>error</level>
    </logging>
    <transformations>
        <template name="clean"/>
    </transformations>
</phpdocumentor>
